#include<nits/stdc++.h>
#include<stack>
using namespace std;

bool compare(char b,char a)
{
    if(a=='(' && b==')' || a=='{' && b=='}' || a=='[' && b==']')
    return true;
    return false;
}

bool balancedparanthesis(string s)
{
    stack<char>st;
    for(int i=0;i<s.size();i++)
    {
        if(s[i]=='(' || s[i]=='{' || s[i]=='[')
        {
            st.push(s[i]);
        }
        else if(s[i]==')' || s[i]=='}' || s[i]==']')
        {
            if(st.empty() || !compare(s[i],st.top()))
            return false;
            else
            st.pop();
        }
        if(st.empty())
        return true;
        return false;
    }
    return true;
}

int main() {
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        if(balancedparanthesis(s))
        cout<<"balanced"<<endl;
        else
        cout<<"not balanced"<<endl;
    }
	return 0;
}
